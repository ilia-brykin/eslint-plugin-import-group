{
  "name": "eslint-plugin-import-group",
  "version": "1.1.1",
  "description": "ESLint plugin for enforcing import grouping and named import sorting",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilia-brykin/eslint-plugin-import-group.git"
  },
  "scripts": {
    "build": "vite build --config vite.config.mjs",
    "pub": "npm publish --scope=@npm",
    "test": "node ./tests/rules/index.js"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "plugin",
    "imports",
    "sorting"
  ],
  "author": "Ilia Brykin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ilia-brykin/eslint-plugin-import-group/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": "./dist/*"
  },
  "homepage": "https://github.com/ilia-brykin/eslint-plugin-import-group#readme",
  "dependencies": {
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "eslint": "9.20.1",
    "vite": "^6.1.0"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "files": [
    "dist",
    "lib",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ]
}
